.game .h5-b {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  padding: 4rpx;
  border: 4rpx solid #879372;
  margin: 0 4rpx 4rpx 0;
}

.game .h5-b.s:after {
  background: #000;
}

.game .h5-b:after {
  content: "";
  display: block;
  width: 24rpx;
  height: 24rpx;
  background: #879372;
  overflow: hidden;
}

page {
  background: #efcc19;
  padding: 0;
  margin: 0;
  font: 20rpx/1 HanHei SC, PingHei, PingFang SC, STHeitiSC-Light, Helvetica Neue, Helvetica, Arial, sans-serif;
  overflow: hidden;
  cursor: default;
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
  -moz-font-feature-settings: "liga", "kern";
  direction: ltr;
  text-align: left
}

.r {
  float: right
}

.l {
  float: left
}

.clear {
  clear: both
}

