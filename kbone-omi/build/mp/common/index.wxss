.h5-h1{
  text-align: center;
}

.ctrl {
  width: 580rpx;
  height: 330rpx;
  margin: 20rpx auto 0;
  position: relative
}

.btn {
  text-align: center;
  color: #111;
  position: absolute;
  white-space: nowrap;
  line-height: 1.6
}

.btn.small {
  font-size: 16rpx
}



.btn .h5-i {
  display: block;
  position: relative;
  border: 1rpx solid #000;
  border-radius: 50%;
  box-shadow: 0 3rpx 3rpx rgba(0, 0, 0, .2)
}

.btn .h5-i:after,
.btn .h5-i:before {
  content: "";
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
  border-radius: 50%;
  box-shadow: inset 0 5rpx 10rpx hsla(0, 0%, 100%, .8)
}

.btn .h5-i:after {
  box-shadow: inset 0 -5rpx 10rpx rgba(0, 0, 0, .8)
}

.btn .h5-i._23aw:before {
  box-shadow: inset 0 -3rpx 6rpx hsla(0, 0%, 100%, .6)
}

.btn .h5-i._23aw:after {
  box-shadow: inset 0 5rpx 5rpx rgba(0, 0, 0, .6)
}

.btn.cm-btn .h5-i {
  background: #5a65f1;
  background: -moz-linear-gradient(top, #6e77ef, #6e77ef)
}

.btn.pp .h5-i {
  background: #2dc421;
  background: -moz-linear-gradient(top, #4bc441, #4bc441)
}

.btn.reset .h5-i {
  background: #dd1a1a;
  background: -moz-linear-gradient(top, #dc3333, #dc3333)
}

.btn.space .h5-i {
  width: 160rpx;
  height: 160rpx
}


.btn .h5-i:active {
  transform: scale(0.9);
}


.btn.cm-btn-dir .h5-i {
  width: 100rpx;
  height: 100rpx
}

.btn.small .h5-i {
  width: 52rpx;
  height: 52rpx;
  box-shadow: 1rpx 1rpx 1rpx rgba(0, 0, 0, .2)
}

.btn.small .h5-i:after,
.btn.small .h5-i:before {
  box-shadow: inset 0 3rpx 6rpx hsla(0, 0%, 100%, .8)
}

.btn.small .h5-i:after {
  box-shadow: inset 0 -3rpx 6rpx rgba(0, 0, 0, .8)
}

.btn.small .h5-i._23aw:before {
  box-shadow: inset 0 -1rpx 2rpx hsla(0, 0%, 100%, .6)
}

.btn.small .h5-i._23aw:after {
  box-shadow: inset 0 3rpx 3rpx rgba(0, 0, 0, .7)
}

.btn.cm-btn-dir .h5-em {
  display: block;
  width: 0;
  height: 0;
  border: 8rpx solid;
  border-color: transparent transparent #111;
  position: absolute;
  top: 50%;
  left: 50%;
  margin: -18rpx 0 0 -8rpx
}


.up {
  top: 0rpx; 
  left: 374rpx;
}

.up .h5-em{
  transform: translate(0rpx, 70rpx) scale(1, 2);
}


.up .h5-span {
  position: absolute;
  top: 5rpx;
  left: 102rpx
}

.down{
  top: 180rpx; 
  left: 374rpx;
}

.down .h5-em{
  transform: translate(0rpx, -71rpx) rotate(180deg) scale(1, 2);
}

.left{
  top: 90rpx; 
  left: 284rpx;
}

.left .h5-em{
  transform: translate(60rpx, -12rpx) rotate(270deg) scale(1, 2);
}

.right{
  top: 90rpx; left: 464rpx;
}

.right .h5-em{
  transform: translate(-60rpx, -12rpx) rotate(90deg) scale(1, 2);
}

.space{
  top: 100rpx; 
  left: 52rpx;
}

.reset{
  top: 0rpx; 
  left: 156rpx;
}

.pp{
  top: 0rpx; 
  left: 60rpx;
}


