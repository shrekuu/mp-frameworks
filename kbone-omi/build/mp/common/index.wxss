.game {

  border: 2rpx solid #000;
  padding: 10rpx 0 5rpx 0;
  width: 725rpx;
  margin: 10rpx auto;
  background-color: #9ead86;
  text-align: center;
}

.game .h5-p {
  width: 725rpx;
  height: 44rpx;
  white-space: nowrap;
  margin: 0;
  padding: 0;
}

